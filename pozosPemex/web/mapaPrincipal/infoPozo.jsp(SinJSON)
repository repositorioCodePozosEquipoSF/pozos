		else if(op == 2)
			$('#btnPR').css('cursor','pointer').attr("src", "/pozosPemex/img/btn_ParamRef_on.jpg");	
		else if(op == 3)
			$('#btnDP').css('cursor','pointer').attr("src", "/pozosPemex/img/btn_DesPerf_on.jpg");	
